{
  "name": "rfc6902-gh-pages",
  "version": "3.0.0",
  "description": "Demo of RFC6902 (https://github.com/chbrown/rfc6902)",
  "homepage": "https://github.com/chbrown/rfc6902/tree/gh-pages",
  "repository": "git://github.com/chbrown/rfc6902.git#gh-pages",
  "author": "Christopher Brown <io@henrian.com> (http://henrian.com)",
  "license": "MIT",
  "dependencies": {
    "angular": "1.4.3",
    "flow-copy": "0.1.2",
    "ngstorage": "0.3.8",
    "rfc6902": "3.0.0"
  },
  "devDependencies": {
    "babel": "5.8.21",
    "babelify": "6.1.3",
    "browserify": "11.0.1",
    "browserify-ngannotate": "1.0.1",
    "clean-css": "3.3.9",
    "less": "3.8.1",
    "watchify": "3.3.1",
    "watsh": "0.1.0"
  },
  "scripts": {
    "css": "lessc site.less | cleancss --keep-line-breaks --skip-advanced -o site.css",
    "bundle": "browserify app.js -t babelify -t browserify-ngannotate -o bundle.js",
    "bundle-min": "closure-compiler --angular_pass --language_in ECMASCRIPT5 --warning_level QUIET bundle.js > bundle.min.js",
    "prepare": "npm run bundle && npm run bundle-min && npm run css",
    "start": "watchify app.js -t babelify -t browserify-ngannotate -o bundle.js -v"
  }
}
